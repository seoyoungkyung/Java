package ex04;

public class 중괄호블럭 {

	public static void main(String[] args) {
		
		int n = 100;
		n = 200;
		
		int kor;	//변수 선언(MAIN안 전체에서 사용가능)
		kor = 100;
		System.out.println(kor);	//변수 사용
		
		
		{
			//블럭 안 내용은 블럭 내에서만 사용가능
			int num = 10;
			System.out.println(num);
		}

		{
			int num2 =20;
			System.out.println(num2);
		}
		
		{
			int num2 =30;
			System.out.println(num2);
			
		}
	}

}
